<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand col-9" href="#">Admin Panel</a>


  <div class="collapse navbar-collapse col-3" id="navbarSupportedContent">
    <form class="form-inline my-2 my-lg-0" action="/admin/search-user" method="post">
      <input class="form-control mr-sm-2" type="search" placeholder="Search" name="name" aria-label="Search">
      <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
        <a class="btn btn-danger" href="/admin/create-user" >Create user</a>

    </form>
  </div>
</nav>
<div class="container">

<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Contact</th>
      <th scope="col">Action</th>
    </tr>
    {{#each users}}
    <tr>
      <td scope="col">{{this.name}}</td>
      <td scope="col">{{this.email}}</td>
      <td scope="col">{{this.phone}}</td>
      <td scope="col">
        <a class="btn btn-primary" href="/admin/edit/{{this._id}}">Edit</a>
        <a class="btn btn-danger" href="/admin/delete-user/{{this._id}}">Delete</a>
      </td>
    </tr>
    {{/each}}
  </thead>
  <tbody>
   
  </tbody>
</table>

<div class=" d-flex mt-4 justify-content-center">
<a class="btn  btn-danger" href="/admin/logout">Logout</a>
</div>
</div>
